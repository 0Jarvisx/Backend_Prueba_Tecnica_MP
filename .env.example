# Server Configuration
NODE_ENV=development
PORT=3000

# SQL Server Database Configuration
DATABASE_URL="sqlserver://localhost:1433;database=PruebaTecnicaMP;user=sa;password=YourPassword123;encrypt=true;trustServerCertificate=true"

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
